{
  "name": "web",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "clear": "rm -rf node_modules .cache functions public/build pnpm-lock.yaml",
    "update": "pnpm update -i -r -L",
    "build": "remix build",
    "dev:remix": "remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development npm run wrangler",
    "dev": "npm-run-all build --parallel \"dev:*\"",
    "start": "cross-env NODE_ENV=production npm run wrangler",
    "typecheck": "tsc",
    "wrangler": "wrangler pages dev ./public",
    "types": "supabase gen types typescript --local > app/lib/database.types.ts"
  },
  "packageManager": "pnpm@8.6.1",
  "dependencies": {
    "@headlessui/react": "^1.7.15",
    "@heroicons/react": "^2.0.18",
    "@remix-run/cloudflare": "^1.19.0",
    "@remix-run/cloudflare-pages": "^1.19.0",
    "@remix-run/react": "^1.19.0",
    "@supabase/auth-helpers-remix": "^0.2.1",
    "@tsndr/cloudflare-worker-jwt": "^2.2.1",
    "clsx": "^2.0.0",
    "isbot": "^3.6.13",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-qr-code": "^2.0.11"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.19.0",
    "@remix-run/dev": "^1.19.0",
    "@remix-run/eslint-config": "^1.19.0",
    "@tailwindcss/typography": "^0.5.9",
    "@types/react": "~18.2.15",
    "@types/react-dom": "^18.2.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.45.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "wrangler": "^3.3.0"
  },
  "engines": {
    "node": ">=18"
  }
}
